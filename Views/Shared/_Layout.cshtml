    <!--Boostrap-->


</head>
<body>
    <div class="navbar navbar-dark bg-dark">
        <div class="container">
            <div class="navbar-header">
                @Html.ActionLink("Kurumlar | Nazım ÖZTÜRK", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - Nazım ÖZTÜRK | Kurumlar</p>
        </footer>
    </div>
    @RenderSection("scripts", required: false)
</body>
</html>
